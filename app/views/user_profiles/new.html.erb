<h1>Sign Up</h1>

<%# name / bio %>

<%= form_for @user_profile do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>
    <%= f.label :bio %>
    <%= f.text_area :bio %><br><br>
    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.submit "Create New User"%>
<% end %>